<Window x:Class="LKtunnel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LKtunnel - VPN Client" Height="400" Width="600">
    <Grid>
        <StackPanel>
            <Button Name="DashboardButton" Content="Dashboard" Click="Dashboard_Click"/>
            <Button Name="OpenVPNButton" Content="OpenVPN" Click="OpenVPN_Click"/>
            <Button Name="WireGuardButton" Content="WireGuard" Click="WireGuard_Click"/>
            <Button Name="ShadowsocksButton" Content="Shadowsocks" Click="Shadowsocks_Click"/>
            <Button Name="V2RayButton" Content="V2Ray" Click="V2Ray_Click"/>
            <Button Name="SSHButton" Content="SSH Tunneling" Click="SSH_Click"/>
            <Button Name="LogsButton" Content="Logs" Click="Logs_Click"/>
        </StackPanel>

        <ContentControl x:Name="MainContent" Margin="150,0,0,0"/>
    </Grid>
</Window>
