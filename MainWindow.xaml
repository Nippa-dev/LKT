<Window x:Class="LKtunnel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LKtunnel - VPN Client" Height="250" Width="400">
    <Grid>
        <Button Name="ConnectButton" Content="Connect VPN" Width="150" Height="40"
                HorizontalAlignment="Center" VerticalAlignment="Center" Click="ConnectVPN_Click"/>
        <Button Name="DisconnectButton" Content="Disconnect VPN" Width="150" Height="40"
                HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20"
                Click="DisconnectVPN_Click"/>
    </Grid>
</Window>
